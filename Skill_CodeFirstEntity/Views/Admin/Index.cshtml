@using Skill_CodeFirstEntity.Models.Siniflar
@model List<YETENEKLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
}
<br />
<table class="table table-bordered">
   <tr>
       <th>ID</th>
       <th>YETENEK</th>
       <th>DEĞER</th>
       <th>SİL</th>
       <th>GÜNCELLE</th>
   </tr>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.ID</td>
            <td>@p.ACIKLAMA</td>
            <td>@p.DEGER</td>
            <td><a href="/Admin/Sil/@p.ID" class="btn btn-danger">SİL</a></td>
            <td><a href="/Admin/Güncelle/@p.ID" class="btn btn-success">GÜNCELLE</a></td>

        </tr>

    }

</table>
